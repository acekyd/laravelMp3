{
    "name": "acekyd/laravelmp3",
    "description": "A light weight Laravel package to handle simple operations with mp3 files",
    "keywords": ["laravel", "mp3", "audio", "media" "ID3"],
    "homepage": "https://github.com/acekyd/laravelmp3",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Abati AceKYD Adewale",
            "email": "acekyd01@gmail.com",
            "role": "Developer",
            "homepage": "http://acekyd.com"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "illuminate/support": "~5"
    },
    "autoload": {
        "psr-4": {
            "Acekyd\\LaravelMP3\\": "src/"
        }
    },
    "require": {
        "php": "^5.3.3 || ^7.0"
    },
     "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Acekyd\\LaravelMP3\\LaravelMP3ServiceProvider"
            ],
            "aliases": {
                "LaravelMp3": "Acekyd\\LaravelMP3\\LaravelMP3Facade"
            }
        }
    }
}
